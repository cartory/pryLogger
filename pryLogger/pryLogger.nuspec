<?xml version="1.0" encoding="utf-8"?>
<package >
  <metadata>
    <id>pryLogger</id>
    <version>1.0.0</version>
    <title>pryLogger</title>
    <authors>cartory</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <!-- <icon>icon.png</icon> -->
    <projectUrl>https://github.com/cartory/pryLogger.git</projectUrl>
    <description>
      This library provides logging and event capabilities. 
      It enables logging to files and the console, event management for REST APIs and databases, and database connection management. 
      Additionally, it includes extensions for XML handling within a REST API
    </description>
    <releaseNotes>
      Version 1.0.0:
      - Added logging functionality to files and the console.
      - Implemented event management for REST APIs and databases.
      - Introduced database connection management.
      - Included extensions for XML handling within a REST API.
    </releaseNotes>
    <copyright>$copyright$</copyright>
    <tags>Logs LogsAttributes FileLog ConsoleLog Events RestEventLogs RestXmlExtensions DbEventLogs DbConnectionManager</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="MrAdvice" version="2.9.9" />
        <dependency id="Newtonsoft.Json" version="13.0.3" />
      </group>
    </dependencies>
  </metadata>
</package>